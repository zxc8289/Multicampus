<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".InsertActivity" tools:layout_editor_absoluteY="-12dp" tools:layout_editor_absoluteX="-8dp">

    <EditText
            android:layout_width="411dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/insertJob"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/insertRelationship"
            android:layout_marginTop="44dp" android:layout_marginStart="32dp"/>
    <TextView
            android:text="직업"
            android:layout_width="85dp"
            android:layout_height="36dp" android:id="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" android:layout_marginTop="44dp"/>
    <TextView
            android:text="나이"
            android:layout_width="85dp"
            android:layout_height="36dp" android:id="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="52dp" android:layout_marginStart="36dp"/>
    <EditText
            android:layout_width="411dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/insertAge"
            app:layout_constraintTop_toBottomOf="@+id/insertName"
            app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginTop="44dp"
            android:layout_marginStart="32dp"/>
    <EditText
            android:layout_width="411dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/insertName"
            app:layout_constraintStart_toEndOf="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="80dp" android:layout_marginStart="28dp"/>
    <EditText
            android:layout_width="411dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/insertRelationship"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/insertAge"
            android:layout_marginTop="56dp"
            android:layout_marginStart="32dp"/>
    <TextView
            android:text="이름"
            android:layout_width="85dp"
            android:layout_height="36dp" android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.069" app:layout_constraintVertical_bias="0.095"/>
    <TextView
            android:text="관계"
            android:layout_width="85dp"
            android:layout_height="36dp" android:id="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="64dp" android:layout_marginStart="36dp"/>
    <EditText
            android:layout_width="411dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/insertAddress"
            app:layout_constraintStart_toEndOf="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/insertJob"
            android:layout_marginStart="32dp" android:layout_marginTop="36dp"/>
    <EditText
            android:layout_width="411dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/insertPhone"
            app:layout_constraintStart_toEndOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/insertAddress"
            android:layout_marginTop="48dp" android:layout_marginStart="32dp"/>
    <TextView
            android:text="전화번호"
            android:layout_width="85dp"
            android:layout_height="36dp" android:id="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" android:layout_marginTop="56dp"/>
    <TextView
            android:text="주소"
            android:layout_width="85dp"
            android:layout_height="36dp" android:id="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/textView8" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" android:layout_marginTop="52dp"/>
    <Button
            android:text="추가"
            android:layout_width="189dp"
            android:layout_height="63dp" android:id="@+id/insert"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintVertical_bias="0.828"/>
</androidx.constraintlayout.widget.ConstraintLayout>